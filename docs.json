{
  "$schema": "https://mintlify.com/schema.json",
  "theme": "mint",
  "name": "Autonoma",
  "logo": {
    "dark": "/logo-hd-white.svg",
    "light": "/logo-hd-dark.svg"
  },
  "favicon": "/logo-hd-dark.svg",
  "colors": {
    "primary": "#4f46e5",
    "light": "#4f46e5",
    "dark": "#4f46e5"
  },
  "background": {
    "decoration": "gradient",
    "color": {
      "dark": "#0d0d0d"
    }
  },
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:info@autonoma.app"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://autonoma.app"
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "icon": "globe",
          "href": "https://autonoma.app/"
        },
        {
          "anchor": "Feature Requests",
          "icon": "pencil",
          "href": "https://autonoma.bettermode.io/"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Get Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["introduction", "getting_started"]
          },
          {
            "group": "Creating Tests",
            "pages": [
              "applications",
              "build-for-ios-apps",
              "new_test",
              "writing_tests"
            ]
          },
          {
            "group": "Running Tests",
            "pages": ["running_tests", "cicd_integration", "slack-integration"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Endpoints",
            "openapi": {
              "source": "openapi.yaml",
              "directory": "api-reference"
            }
          }
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/autonoma-ai/"
    },
    "links": [
      {
        "header": "Product",
        "items": [
          {
            "label": "Autonoma",
            "href": "https://autonoma.app/"
          }
        ]
      }
    ]
  }
}
